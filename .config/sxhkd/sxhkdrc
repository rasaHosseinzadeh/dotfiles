    dmenu_run -i -l 10
super + alt + shift + n
    dunsthist


XF86Audio{Prev,Next}
	mpc -q {prev,next}
shift + XF86Audio{Prev,Next}
	mpc seek {-5,+5}
XF86AudioPlay
    mpc toggle

XF86AudioMute
    pactl set-sink-mute @DEFAULT_SINK@ toggle; kill -51 $(pidof dwmblocks)
XF86AudioRaiseVolume
    pactl set-sink-volume @DEFAULT_SINK@ +5%; kill -51 $(pidof dwmblocks)
XF86AudioLowerVolume
    pactl set-sink-volume @DEFAULT_SINK@ -5%; kill -51 $(pidof dwmblocks)
shift + XF86AudioRaiseVolume
    pactl set-sink-volume @DEFAULT_SINK@ +1%; kill -51 $(pidof dwmblocks)
shift + XF86AudioLowerVolume
    pactl set-sink-volume @DEFAULT_SINK@ -1%; kill -51 $(pidof dwmblocks)
XF86MonBrightnessUp
    xbacklight -inc 5; kill -50 $(pidof dwmblocks)
XF86MonBrightnessDown
    xbacklight -dec 5; kill -50 $(pidof dwmblocks)
shift + XF86MonBrightnessUp
    xbacklight -inc 1; kill -50 $(pidof dwmblocks)
shift + XF86MonBrightnessDown
    xbacklight -dec 1; kill -50 $(pidof dwmblocks)
